{
  "declare_desktop": {
    "body": [
      "@include media-query-up(xl) {",
      "  ${1}",
      "}"
    ],
    "prefix": "declare_desktop"
  },
  "declare_font": {
    "body": "@include koala-typography-${1:paragraph}-${2:size};",
    "prefix": "declare_font"
  },
  "declare_tablet": {
    "body": [
      "  @include media-query-up(md) {",
      "    ${1}",
      "  }"
    ],
    "prefix": "declare_tablet"
  }
}
